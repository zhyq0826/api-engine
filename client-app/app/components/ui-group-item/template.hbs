<div class="ui selection list">
    <div class="item"  style="position:relative" {{action "toggleContent"}} >
        <i class="folder {{if isOpenFolder 'open' ''}} icon"></i>
        <div class="content"  >
            <div class="header">
                {{model.name}}
            </div>
        </div>
        <i class="setting icon" style="position:absolute;top:5px;right:10px;" ></i>
    </div>
</div>
<div class="ui selection list js-content" >
    <div class="item">
        <div class="content">
            {{#link-to "api-dashboard.api-detail"  class="green" }}
                Get
                <span>Get TODO</span>
            {{/link-to}}
        </div>
    </div>
    <div class="item">
        <div class="content">
            {{#link-to "api-dashboard.api-detail"  class="grey" }}
                Post
                <span>Post TODO</span>
            {{/link-to}}
        </div>
    </div>
    <div class="item">
        <div class="content">
            {{#link-to "api-dashboard.api-detail"  class="blue" }}
                Update
                <span>Update TODO</span>
            {{/link-to}}
        </div>
    </div>
    <div class="item">
        <div class="content">
            {{#link-to "api-dashboard.api-detail" class="red" }}
                Delete
                <span>Delete TODO</span>
            {{/link-to}}
        </div>
    </div>
</div>
